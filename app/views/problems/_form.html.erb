<%= render 'shared/error_messages' %>

<%= form_for @problem, url: problems_path, method: :post, html: { multipart: true } do |f| %>
	 <div class="form-group">
		<%= f.label :name %>
		<%= f.text_field :name, class: 'form-control' %>
	</div>
	<div class="form-group">
	  	<%= f.submit 'Add board', class: 'btn btn-primary'%>
	</div>
<% end %>
